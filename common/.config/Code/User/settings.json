{
  "vim.easymotion": true,
  "vim.handleKeys": {
    "<C-p>": false
  },
  "editor.minimap.enabled": false,
  "vim.leader": "Space",
  "editor.detectIndentation": false,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["Space", "w"], // 検索するためのキーをfから変えたい場合は､ここを変更する
      "after": ["leader", "leader", "w"]
    },
    {
      "before": ["Space", "b"],
      "after": ["leader", "leader", "b"]
    },
    {
      "before": ["Space", "e"],
      "after": ["leader", "leader", "e"]
    },
    {
      "before": ["Space", "g", "e"],
      "after": ["leader", "leader", "g", "e"]
    }
  ],
  "[json]": {
    "editor.formatOnSave": false
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "[graphql]": {
    "editor.formatOnSave": false
  },
  "[go]": {
    "editor.defaultFormatter": "golang.go"
  },

  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.userWords": ["Intelli", "Qiita", "redmine"],
  "editor.suggestSelection": "first",
  "trailing-spaces.trimOnSave": true,
  "files.insertFinalNewline": true,
  "editor.formatOnSave": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
  "vscode-neovim.useCtrlKeysForNormalMode": true,
  "vscode-neovim.useCtrlKeysForInsertMode": true,
  "vscode-neovim.textDecorationsAtTop": true,
  "auto-rename-tag.activationOnLanguage": ["*"],
  "go.toolsManagement.autoUpdate": true,
  "editor.scrollBeyondLastLine": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.codeActionsOnSave": {
    "source.addMissingImports": true
  },
  "html.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "javascript.autoClosingTags": false,
  "auto-close-tag.enableAutoCloseTag": false
}
