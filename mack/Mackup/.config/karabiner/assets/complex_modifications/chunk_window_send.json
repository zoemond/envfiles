{
  "title": "chunkwm send",
  "rules": [
    {
      "description": "SEND: fn + option + command + {n,p,index,}",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "n",
            "modifiers": { "mandatory": [ "fn", "option","command" ] }
          },
          "to": [
              {
                 "shell_command":"/usr/local/bin/chunkc tiling::window --send-to-desktop next"
              }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "p",
            "modifiers": { "mandatory": [ "fn", "option","command" ] }
          },
          "to": [
              {
