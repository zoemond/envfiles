{
  "title": "WINDOW MOVE",
  "rules": [
    {
      "description": "WINDOW: SWAP [ fn + option + {h,j,k,l} ]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": { "mandatory": [ "fn", "option" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --swap west" 
	      }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": { "mandatory": [ "fn", "option" ] }
          },
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --swap south" 

	      }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": { "mandatory": [ "fn", "option" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --swap north" 
	      }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": { "mandatory": [ "fn", "option" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --swap east" 
	      }
          ]
        }
      ]
    },
    {
      "description": "WINDOW: WARP [ fn + command + {h,j,k,l}]",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "h",
            "modifiers": { "mandatory": [ "fn", "command" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --warp west" 
	      }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "j",
            "modifiers": { "mandatory": [ "fn", "command" ] }
          },
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --warp south" 

	      }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "k",
            "modifiers": { "mandatory": [ "fn", "command" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --warp north" 
	      }
          ]
        },
        {
          "type": "basic",
          "from": {
            "key_code": "l",
            "modifiers": { "mandatory": [ "fn", "command" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/yabai -m window --warp east" 
	      }
          ]
        }
      ]
    }
  ]
}
