# Git
abbr gst "git status"
abbr gss "git stash"
# %an: autor
# %C: 次の要素のみため(<color>, bold, reverseなど)指定
abbr glo 'git log --graph --color=always --format="%C(magenta)%an% C(auto)%h%d %s %C(black)%C(bold)%cr"'
abbr ga "git add"
abbr gaa "git add -A"
abbr gbr "git branch"
abbr gf "git fetch"
abbr gdf "git diff"
abbr gdn "git diff --name-only"
abbr gca "git commit -a -m "
abbr gcm "git commit  -m "
abbr gpk "git cherry-pick"
abbr gpc "git cherry-pick --continue"
abbr grb "git rebase"
