{
  "title": "chunkwm focus window",
  "rules": [
    {

      "description": "FOCUS DESKTOP: fn + control + {index}",
      "manipulators": [
        {
          "type": "basic",
	  "from": {
              "key_code": "1",
              "modifiers": { "mandatory": [ "fn", "control" ] }
            },
            "to": [
              { "shell_command": "/usr/local/bin/chunkc tiling::desktop --focus 1" }
            ] 
        }, 
        {
          "type": "basic",
	  "from": {
              "key_code": "2",
