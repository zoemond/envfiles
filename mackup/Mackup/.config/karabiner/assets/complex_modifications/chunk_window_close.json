{
  "title": "chunkwm mode",
  "rules": [
    {
      "description": "CLOSE: fn + w",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "w",
            "modifiers": { "mandatory": [ "fn" ] }
          }, 
          "to": [
	      {
                 "shell_command":"/usr/local/bin/chunkc tiling::window --close" 
	      }
          ]
        }
      ]
    }
  ]
}
